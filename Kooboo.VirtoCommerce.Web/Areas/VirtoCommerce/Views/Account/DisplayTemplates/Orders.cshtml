@model VirtoCommerce.Foundation.Orders.Model.Order[]

<table class="data-table" id="my-orders-table">
	<colgroup>
		<col width="1">
		<col width="1">
		<col />
		<col width="1">
		<col width="1">
		<col width="1">
	</colgroup>
	<thead>
		<tr class="first last">
			<th>@("Order #".LocalizeHtml())</th>
			<th>@("Date".LocalizeHtml())</th>
			<th>@("Ship To".LocalizeHtml())</th>
			<th><span class="nobr">@("Order Total".LocalizeHtml())</span></th>
			<th><span class="nobr">@("Order Status".LocalizeHtml())</span></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	<tbody>
		@for (var i = 0; i < Model.Count(); i++)
  {
			@Html.DisplayFor(m => m[i], "OrderLine")
  }
	</tbody>
</table>
